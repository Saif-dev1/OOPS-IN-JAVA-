class Animal{

    String name;
    int age;
    String gender;

    public Animal(){
        name="";
        age=0;
        gender="";
    }

    public Animal(String name, int age, String gender) {
        this.name = name;
        this.age = age;
        this.gender = gender;
    }

    void produceSound(){

    }
}
class Dog extends Animal{

    Dog(){
        name="";
        age=0;
        gender="";
    }
    Dog(String name,int age,String gender){
        super(name,age,gender);
    }
    void produceSound(){
        System.out.println("Bhawo,Bhawo");
    }
}

class Frog extends Animal{
    Frog(){
        name="";
        age=0;
        gender="";
    }
    Frog(String name,int age,){
        super(name,age,gender);
        
    }
    void produceSound(){
        System.out.println("Tran,Tran");
    }
}

